              <widget class="pitch_with_tactics" id="pitc" player_icon_file="" icon_flags="show_name,show_extra_details,use_overview_icons,drop_initial_if_possible,show_position_role_duty_suitability,show_selection_image,show_number,show_role_duty_labels,show_familiarity" pitch_vertical_padding="0" vertical="true" pitch_player_icon_text_size="5" pitch_alignment="can_scale" maximum_player_size="0.22" pitch_line_colour="text" ncol="tactics normal run" dcol="tactics darkened run" gcol="tactics dragged run" pitch_draw_grass="false" pitch_base_appearance="" pitch_pattern="ui circles" pitch_touchline_width_percentage="20" icon_layout_state_id="st07" line_width="1/4" draw_analysis_overlay="true" analysis_mode="2" pitch_perspective="0.100000"/>

