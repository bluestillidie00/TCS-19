        <translation id="text" translation_id="450768" type="use" value="Current Tactic Analysis[COMMENT: title of subsection; showing the current tactic's analysis in terms of areas of strenghts, weaknesses and partnership]" />
      </widget>

      <container>
        <layout class="arrange_horizontal_attachment" layout="-1,-1, -1" offset="0" />
        <layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true" />

        <container>
          <layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true" />
          <layout class="arrange_horizontal_attachment" alignment="middle" offset="0" />

          <container class="team_tactics_overview_panel" file="none" width="206">
            <layout class="centre_in_parent_attachment" alignment="all" offset="0" apply_to_children="true" />

