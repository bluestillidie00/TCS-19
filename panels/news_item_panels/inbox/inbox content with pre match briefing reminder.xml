                      ncol="tactics normal run"
                      dcol="tactics darkened run"
                      gcol="tactics dragged run"
                      pitch_draw_grass="false"
                      pitch_base_appearance=""
                      pitch_pattern="ui circles"
                      pitch_touchline_width_percentage="20"
                      icon_layout_state_id="st07"
                      line_width="1.4"
                      draw_analysis_overlay="true"
                      analysis_mode="1"
                      pitch_perspective="0.100000" />

